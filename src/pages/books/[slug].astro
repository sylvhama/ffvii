---
import Post from "@components/Post.astro";
import { getEnglishStaticPaths } from "@utils";

export async function getStaticPaths() {
  return await getEnglishStaticPaths("books")();
}

const {
  post: { id, data, render },
} = Astro.props;
const { Content } = await render();
---

<Post id={id} data={data} collection="books">
  <Content />
</Post>
