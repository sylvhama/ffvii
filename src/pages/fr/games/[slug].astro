---
import Post from "@components/Post.astro";
import { getFrenchStaticPaths } from "@utils";

export async function getStaticPaths() {
  return await getFrenchStaticPaths("games")();
}

const {
  post: { id, data, render },
} = Astro.props;
const { Content } = await render();
---

<Post id={id} data={data} collection="games">
  <Content />
</Post>
